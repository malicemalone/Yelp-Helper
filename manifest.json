{
  "manifest_version": 2,

  "name": "Yelp-Helper",
  "description": "This extension sorts user-submitted photos by date for an improved UX",
  "version": "1.0",

  "background": {
  "scripts": ["jquery-1.12.0", "background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }, 

  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "32": "icon32.png",
             "128": "icon128.png" }

  // "permissions": [
  //   "activeTab",
  //   "https://ajax.googleapis.com/"
  // ]
}
